{
  "name": "<%= projectname %>",
  "private": true,
  "version": "0.0.1",
  "dependencies": {<% if (legacy) { %>
    "modernizr": "^3.3.1",
    "es5-shim": "^4.5.3",<% } %><% if (includeJQuery) { %>
    "jquery": "<% if (legacy) { %>1.2.0<% } else { %>2.2.0<% } %>",<% } %>
    "normalize.css": "^3.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "browser-sync": "^2.11.1",
    "del": "^2.2.0",
    "gulp": "^3.9.0",
    "gulp-base64": "^0.1.3",
    "gulp-concat": "^2.6.0",
    "gulp-htmlmin": "^1.3.0",
    "gulp-if": "^2.0.0",
    "gulp-inline-source": "^2.1.0",
    "gulp-load-plugins": "^1.2.0",
    "gulp-csso": "^1.0.1",
    "gulp-postcss": "^6.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-useref": "^3.0.5",
    "gulp-replace": "^0.5.4",<% if (useRev) { %>
    "gulp-rev": "^6.0.1",
    "gulp-rev-replace": "^0.4.3",<% } %>
    "gulp-sass": "^2.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-imagemin": "^2.4.0",<% if (useSpritesmith) { %>
    "gulp.spritesmith": "^6.2.0",<% } %>
    "gulp-uglify": "^1.5.1",<% if (useProxy) { %>
    "http-proxy-middleware": "^0.10.0",<% } %>
    "lodash": "^4.2.1",
    "merge-stream": "^1.0.0",
    "run-sequence": "^1.1.5"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  <% if (useProxy) { %>"proxyAddr": "http://127.0.0.1:3000",<% } %>
  "browsers": [
    "Explorer >= 9",
    "Firefox >= 24",
    "Chrome >= 31",
    "Safari >= 6",
    "Opera >= 23",
    "iOS >= 6",
    "Android >= 4.0",
    "and_uc >= 9",
    "and_ff >= 30",
    "and_chr >= 31",
    "ie_mob >= 10",
    "bb >= 10",
    "op_mob >= 10"
  ],
  "src": {
    "html": {
      "dir": "src",
      "ext": ".html"
    },
    "css": {
      "dir": "src/css",
      "ext": ".css"
    },
    "scss": {
      "dir": "src/scss",
      "ext": ".scss"
    },
    "js": {
      "dir": "src/js",
      "ext": ".js"
    },
    "img": {
      "dir": "src/img",
      "ext": ".{png,jpg,gif}"
    }
  },
  "htmlmin": {
    "collapseWhitespace": true,
    "collapseBooleanAttributes": true,
    "removeEmptyAttributes": true,
    "removeComments": true,
    "minifyJS": true,
    "minifyCSS": true
  }
}
